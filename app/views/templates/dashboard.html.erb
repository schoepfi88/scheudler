<section class="hidden-print">
  <h1 class="page-header"><span class="fa fa-info-circle"></span> <%= t('.page_header') %></h1>
</section>
<div class="row">
  <div class="col-lg-4 col-xs-12 col-md-6">
    <!-- next events -->
    <div class="panel panel-default box-dashboard">
      <div class="panel-heading">
        <span class="fa fa-trophy"></span>
         Next events
      </div>
      <!-- next events -->
      <div class="panel-body">
        <!-- next event 1 -->
        <div class="panel panel-default inbox-dashboard">
          <%= image_tag("jp-rodgers-articleLarge.jpg", :class => "img-circle") %>
          MON 15. dec 5pm 
          <!-- cancel botton -->
          <div class="btn-group pull-right" style="margin: 5px">
            <div class="btn">
              <span class="fa fa-close"></span>
            </div>
          </div>
        </div>
        <!-- next event 2-->
        <div class="panel panel-default inbox-dashboard">
          <%= image_tag("jp-rodgers-articleLarge.jpg", :class => "img-circle") %>
          WED 17. dec 6pm 
          <!-- cancel botton -->
          <div class="btn-group pull-right" style="margin: 5px">
            <div class="btn">
              <span class="fa fa-close"></span>
            </div>
          </div>
        </div>
      </div> <!-- end next events -->
    </div>
  </div>
  <!-- My groups -->
  <div class="col-lg-4 col-xs-12 col-md-6">
    <div class="panel panel-default box-dashboard">
      <div class="panel-heading">
        <span class="fa fa-group"></span> 
        My Groups
      </div>
      <!-- groups -->
      <div class="panel-body">
        <div class="panel panel-default inbox-dashboard">
          <%= image_tag("jp-rodgers-articleLarge.jpg", :class => "img-circle") %>
          Green Bay Packers
        </div>
        <div class="panel panel-default inbox-dashboard">
          <%= image_tag("favicon.png", :class => "img-circle") %>
          Scheudlers
        </div>
      </div>
      <!-- end groups -->
    </div>
  </div>
  <!-- New Invites -->
  <div class="col-lg-4 col-xs-12 col-md-6">
    <div class="panel panel-default box-dashboard">
      <div class="panel-heading">
        <span class="fa fa-envelope-o"></span> 
        My Invites
      </div>
      <div class="panel-body">
        <!-- invite 1 -->
        <div class="panel panel-default inbox-dashboard">
          <%= image_tag("jp-rodgers-articleLarge.jpg", :class => "img-circle") %>
          every Monday 5pm Training
          <!-- confirm bottons -->
          <div class="btn-group pull-right" style="margin: 5px">
            <div class="btn">
              <span class="fa fa-check"></span>
            </div>
            <div class="btn">
              <span class="fa fa-close"></span>
            </div>
          </div>
        </div>
        <div class="panel panel-default inbox-dashboard">
          <%= image_tag("jp-rodgers-articleLarge.jpg", :class => "img-circle") %>
          every Saturday 6pm Game
          <!-- confirm bottons -->
          <div class="btn-group pull-right" style="margin: 5px">
            <div class="btn">
              <span class="fa fa-check"></span>
            </div>
            <div class="btn">
              <span class="fa fa-close"></span>
            </div>
          </div>
        </div>
      </div>  
    </div>
  </div>
</div>
<div class="row">
  <!-- Blog of group -->
  <div class="col-lg-4 col-xs-12 col-md-6">
